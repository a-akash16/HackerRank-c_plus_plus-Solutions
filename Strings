#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
    
    string a,b;
    
    cin>>a>>b;
    
    int len_a=a.size();
    int len_b=b.size();
    
    cout<<len_a<<" "<<len_b<<endl;
    cout<<a<<b<<endl;
    
    char c=a[0];
    char d=b[0];
    
    a[0]=d;
    b[0]=c;
    
    cout<<a<<" "<<b;
    
       
    return 0;
}
